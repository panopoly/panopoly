{
    "name": "panopoly/panopoly",
    "description": "Panopoly is a powerful Drupal base-distribution with a powerful editorial experience",
    "type": "drupal-profile",
    "license": "GPL-2.0-or-later",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "bower-asset/dropzone": "5.7.2",
        "cweagans/composer-patches": "^1.6.5",
        "drupal-ckeditor-plugin/colorbutton": "4.*",
        "drupal-ckeditor-plugin/fakeobjects": "4.*",
        "drupal-ckeditor-plugin/panelbutton": "4.*",
        "drupal-ckeditor-plugin/tabletoolstoolbar": "^0.0.1",
        "drupal/ckeditor_drupalbreaks": "^2.0.0",
        "drupal/colorbutton": "~1.1",
        "drupal/config_actions": "~1.0-beta4",
        "drupal/config_update": "~1.5",
        "drupal/core": "^8.8 || ^9",
        "drupal/ctools": "~3.0",
        "drupal/dropzonejs": "2.x-dev",
        "drupal/editor_advanced_link": "~1.4",
        "drupal/entity_browser": "^2.0",
        "drupal/entity_embed": "^1.0@beta",
        "drupal/fakeobjects": "~1.0",
        "drupal/features": "~3.7",
        "drupal/inline_entity_form": "^1.0@RC",
        "drupal/linkit": "~5.0-beta7",
        "drupal/page_manager": "~4.0-beta3",
        "drupal/panelbutton": "~1.1",
        "drupal/panels": "~4.3",
        "drupal/pathauto": "~1.2",
        "drupal/radix_layouts": "~4.0",
        "drupal/token": "~1.3",
        "oomphinc/composer-installers-extender": "^2"
    },
    "require-dev": {
        "consolidation/robo": "^1.4",
        "spomky-labs/otphp": "^9.0",
        "behat/mink-selenium2-driver": "dev-master"
    },
    "replace": {
        "drupal/panopoly_core": "self.version",
        "drupal/panopoly_images": "self.version",
        "drupal/panopoly_pages": "self.version",
        "drupal/panopoly_theme": "self.version",
        "drupal/panopoly_widgets": "self.version",
        "drupal/panopoly_demo": "self.version",
        "drupal/panopoly_magic": "self.version",
        "drupal/panopoly_media": "self.version",
        "drupal/panopoly_test": "self.version",
        "drupal/panopoly_users": "self.version",
        "drupal/panopoly_wysiwyg": "self.version"
    },
    "extra": {
        "branch-alias": {
            "dev-8.x-2.x": "2.x-dev"
        },
        "patches": {
            "drupal/core": {
                "Issue #2010132: Forum term canonical links": "https://www.drupal.org/files/issues/2020-10-24/2010132-132.patch",
                "Issue #2999235: Image Formatter Links When No Access": "https://www.drupal.org/files/issues/2019-05-02/drupal-image_formatter_links-2999235-7.patch",
                "Issue #2922569: Filtering incorrectly on 'Summary or Trimmed' fields": "https://www.drupal.org/files/issues/2019-12-10/drupal-core-2922569-9.patch",
                "Issue #2653938: Views filter default values overridden": "https://www.drupal.org/files/issues/2021-02-01/drupal-views-filter-default-all-2653938-22.patch",
                "Issue #2958588: Off-canvas style resets are overriding styles": "https://www.drupal.org/files/issues/2021-01-06/off-canvas-style-resets-2958588-13.patch"
            },
            "drupal/entity_browser": {
                "Entity Browser Modal unusable in bartik theme": "https://www.drupal.org/files/issues/bartik-entity-browser.patch",
                "Issue #3160482: Error with UntrustedCallbackException": "https://www.drupal.org/files/issues/2021-03-02/error-with-untrustedcallbackexception-3160482-5.patch"
            }
        }
    }
}