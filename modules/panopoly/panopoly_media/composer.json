{
    "name": "drupal/panopoly_media",
    "description": "Advanced multimedia configuration.",
    "type": "drupal-module",
    "license": "GPL-2.0+",
    "repositories": {
        "asset-packagist": {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    },
    "require": {
        "bower-asset/dropzone": "5.7.2",
        "drupal/core": "^8.8 || ^9",
        "drupal/ctools": "~3.0",
        "drupal/dropzonejs": "2.x-dev",
        "drupal/entity_browser": "^2.0",
        "drupal/entity_embed": "^1.0@beta",
        "drupal/inline_entity_form": "^1.0@RC",
        "drupal/panopoly_images": "2.x-dev",
        "cweagans/composer-patches": "^1.6.5",
        "oomphinc/composer-installers-extender": "^2"
    },
    "extra": {
        "installer-types": [
            "bower-asset",
            "npm-asset"
        ],
        "patches": {
            "drupal/entity_embed": {
                "Issue #3069448: Array to string conversion for Media Image": "https://www.drupal.org/files/issues/2019-10-18/entity-embed-array-to-string--3069448-19.patch"
            },
            "drupal/entity_browser": {
                "Entity Browser Modal unusable in bartik theme": "https://www.drupal.org/files/issues/bartik-entity-browser.patch",
                "Issue #3160482: Error with UntrustedCallbackException": "https://www.drupal.org/files/issues/2021-03-02/error-with-untrustedcallbackexception-3160482-5.patch",
                "Issue #3199269: Trying to access array offset on value of type null in EntityBrowserElement valueCallback": "https://git.drupalcode.org/project/entity_browser/-/merge_requests/7.patch"
            }
        }
    }
}
